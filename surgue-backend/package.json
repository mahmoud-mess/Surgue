{
    "name": "surgue-backend",
    "version": "1.0.0",
    "description": "Backend for the Surgue web game",
    "author": "mahmoud-mess",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "typeorm:run-migrations": "npx typeorm-ts-node-commonjs migration:run -d ./src/typeorm-cli.config.ts",
        "typeorm:generate-migration": "npx typeorm-ts-node-commonjs migration:generate -d ./src/typeorm-cli.config.ts",
        "typeorm:revert-migration": "npx typeorm-ts-node-commonjs migration:revert -d ./src/typeorm-cli.config.ts"
    },
    "dependencies": {
        "@nestjs/class-transformer": "^0.4.0",
        "@nestjs/class-validator": "^0.13.4",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.2.2",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/typeorm": "^10.0.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cookie-parser": "^1.4.6",
        "ioredis": "^5.4.1",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.1",
        "pg": "^8.11.5",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.20",
        "typeorm-naming-strategies": "^4.1.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^4.17.17",
        "@types/node": "^20.3.1",
        "@types/passport-google-oauth20": "^2.0.14",
        "@types/passport-jwt": "^4.0.1",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    }
}
